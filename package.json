{
  "name": "sweepaccess",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn prisma-generate",
    "start": "tsc && node -r ts-node/register -r tsconfig-paths/register dist/src/server.js",
    "dev": "tsx watch --clear-screen=false src/server.ts | pino-pretty",
    "build": "tsc",
    "postbuild": "cp -R ./node_modules/@prisma/client ./dist/node_modules/@prisma/client",
    "lint": "eslint",
    "test": "jest",
    "prisma-migrate": "yarn prisma migrate dev",
    "prisma-generate": "yarn prisma generate",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.7",
    "pino": "^9.3.2",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^22.5.4",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "tsx": "^4.17.0",
    "zod-prisma": "^0.5.4"
  }
}
