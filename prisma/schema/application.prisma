model Application {
  id          String  @id
  name        String
  description String?
  /// @zod.url()
  url         String?
  /// @zod.url()
  avatar      String?
  jsonData    Json? // aca vamos a guardar por ejemplo las organizations de github

  isDeleted Boolean   @default(false)
  createdAt DateTime? @default(now())
  createdBy String?
  updatedAt DateTime? @updatedAt
  updatedBy String?
  deletedAt DateTime?
  deletedBy String?

  ApplicationCollaboratorTenant ApplicationCollaboratorTenant[]
  ApplicationTenant             ApplicationTenant[]
}
