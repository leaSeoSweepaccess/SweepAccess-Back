model Collaborator {
  id        String  @id
  /// @zod.email()
  email     String  @unique
  username  String?
  firstName String?
  lastName  String?
  /// @zod.url()
  avatar    String?

  isDeleted Boolean   @default(false)
  createdAt DateTime? @default(now())
  createdBy String?
  updatedAt DateTime? @updatedAt
  updatedBy String?
  deletedAt DateTime?
  deletedBy String?

  ApplicationCollaborator ApplicationCollaborator[]
  CollaboratorTenant      CollaboratorTenant[]
}
